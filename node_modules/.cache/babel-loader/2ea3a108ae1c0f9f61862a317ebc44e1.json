,CAAgI,sBAAhI,EAAgI;AAAnH,IAAA,EAAE,EAAC,KAAgH;AAAzG,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,OAAA,CAAO,KAAP,CAAF;AAAA,KAAA,CAAoG;AAAlF,mBAAY,SAAsE;AAA5D,sBAAe,SAA6C;AAAlC,IAAA,SAAS,EAAE,KAAA,CAAA,gBAAA,CAAiB;AAAM,GAAhI,E,IAAA,E;;AAAA,I,aAAA,CACI,CAHN,CAQI,EAJJ,mBAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,YAAA,CAAsI,sBAAtI,EAAsI;AAAzH,IAAA,EAAE,EAAC,OAAsH;AAA7G,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,OAAA,CAAO,OAAP,CAAF;AAAA,KAAA,CAAwG;AAApF,mBAAY,SAAwE;AAA9D,sBAAe,SAA+C;AAApC,IAAA,SAAS,EAAE,KAAA,CAAA,gBAAA,CAAiB;AAAQ,GAAtI,E,IAAA,E;;AAAA,I,aAAA,CAEI,EADJ,YAAA,CAAmI,sBAAnI,EAAmI;AAAtH,IAAA,EAAE,EAAC,MAAmH;AAA3G,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,OAAA,CAAO,MAAP,CAAF;AAAA,KAAA,CAAsG;AAAnF,mBAAY,SAAuE;AAA7D,sBAAe,SAA8C;AAAnC,IAAA,SAAS,EAAE,KAAA,CAAA,gBAAA,CAAiB;AAAO,GAAnI,E,IAAA,E;;AAAA,I,aAAA,CACI,CAHN,CAII,CATN,C","sourcesContent":["<template>\n  <div class=\"column\">\n    <div class=\"row\">\n      <game-button id=\"one\" @click=\"theGame('one')\" light-color=\"#44836b\" standard-color=\"aquamarine\" :lightning=\"buttonsHighlight.one\"/>\n      <game-button id=\"two\" @click=\"theGame('two')\" light-color=\"#764e9b\" standard-color=\"#c17fff\" :lightning=\"buttonsHighlight.two\"/>\n    </div>\n    <div class=\"row\">\n      <game-button id=\"three\" @click=\"theGame('three')\" light-color=\"#9b8f4e\" standard-color=\"#ffea7f\" :lightning=\"buttonsHighlight.three\"/>\n      <game-button id=\"four\" @click=\"theGame('four')\" light-color=\"#ad5658\" standard-color=\"#ff7f83\" :lightning=\"buttonsHighlight.four\"/>\n    </div>\n  </div>\n</template>\n\n<script>\nimport GameButton from './GameButton'\nexport default {\n  name: 'ButtonComponent',\n  components: { GameButton },\n  emits: ['getScore'],\n  data () {\n    return {\n      buttonsHighlight: {\n        one: false,\n        two: false,\n        three: false,\n        four: false\n      },\n      myexpect: [],\n      set: ['one', 'two', 'three', 'four'],\n      timer: 0,\n      myTrigger: false,\n      clickedButton: '',\n      counter: 0,\n      score: 0\n    }\n  },\n  props: {\n    trigger: Boolean\n  },\n  updated () {\n  },\n  methods: {\n    gameHardRound (timer) {\n      this.addElement()\n      this.myexpect.forEach((item) => {\n        switch (item) {\n          case 'one':\n            setTimeout(() => this.buttonsHighlight.one = true, timer = timer + 400)\n            setTimeout(() => this.buttonsHighlight.one = false, timer = timer + 400)\n            break\n          case 'two':\n            setTimeout(() => this.buttonsHighlight.two = true, timer = timer + 400)\n            setTimeout(() => this.buttonsHighlight.two = false, timer = timer + 400)\n            break\n          case 'three':\n            setTimeout(() => this.buttonsHighlight.three = true, timer = timer + 400)\n            setTimeout(() => this.buttonsHighlight.three = false, timer = timer + 400)\n            break\n          case 'four':\n            setTimeout(() => this.buttonsHighlight.four = true, timer = timer + 400)\n            setTimeout(() => this.buttonsHighlight.four = false, timer = timer + 400)\n            break\n        }\n      })\n    },\n    addElement () {\n      this.myexpect.push(this.set[Math.ceil(Math.random() * 4 - 1)])\n    },\n    theGame (clickedButton) {\n      if (clickedButton === this.myexpect[this.counter]) {\n        if (this.counter + 1 === this.myexpect.length) {\n          this.score = this.score + 1\n          this.counter = 0\n          // блокируем кнопки\n          this.gameHardRound(0)\n          return this.$emit('getScore', this.score)\n        }\n        this.counter = this.counter + 1\n      }\n      else return this.$emit('getScore', 'Вы проиграли')\n    }\n  },\n  watch: {\n    trigger: function () {\n      this.gameHardRound(0)\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"../assets/commonStyles\";\n#one {\n  border-radius: 100% 0 0 0;\n}\n#two {\n  border-radius: 0 100% 0 0 ;\n}\n#three {\n  border-radius: 0 0 0 100%;\n}\n#four {\n  border-radius: 0 0 100% 0 ;\n}\n</style>\n"],"sourceRoot":""}]}